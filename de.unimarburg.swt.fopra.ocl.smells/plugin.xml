<modelsmell id="de.pum.swt.ocl.mus" modelsmell_description="The variable 'self' is multiply used within an invariant." modelsmell_finderclass="org.eclipse.emf.refactor.modelsmell.MultipleUsagesOfSelf" modelsmell_metamodel="http://www.eclipse.org/ocl/3.1.0/CompleteOCLCST" modelsmell_name="Multiple Usages of Self">
